package aplication;


import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);
		
		System.out.println("Quantos n√∫meros vc quer digitar??");
		int n= sc.nextInt();
		
		int[] vect=new int[n];
		
		for (int i=0;i<vect.length;i++) {
			vect[i]=sc.nextInt();
			
		}
		
		for (int i=0;i<vect.length;i++) {
			if(vect[i]<0) {
				System.out.printf("Numeros negativos:%.2f",vect[i]);
			}
		}
			
	
		
		
		
		sc.close();
	}

}
